{
  "name": "mutent",
  "version": "0.2.0",
  "description": "Keep business logic and other code separated",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "business-logic",
    "crud",
    "entity",
    "immutable"
  ],
  "scripts": {
    "test:ts": "tsc --noEmit",
    "test": "npm run test:ts",
    "build:types": "tsc --emitDeclarationOnly --declaration",
    "build:js": "tsc",
    "build": "npm run build:types && npm run build:js",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mutent.git"
  }
}
